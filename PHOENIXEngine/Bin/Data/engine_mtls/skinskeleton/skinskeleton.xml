					<p param="SrcBlend" value="SBM_SRC_ALPHA"/>
					<p param="DstBlend" value="DBM_ONE_MINUS_SRC_ALPHA"/>
					<p param="CompareEnabled" value="false"/>
					<p param="Compare" value="CM_GEQUAL"/>
